<?xml version="1.0" encoding="utf-8"?>
<!-- item_complaint.xml -->
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/background_gradient">

    <TextView
            android:text="  "
            android:layout_width="401dp"
            android:layout_height="641dp"
            android:id="@+id/textView14"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="@dimen/_300sdp"
            android:layout_height="@dimen/_470sdp"
            android:background="@color/white"
            app:layout_constraintEnd_toEndOf="@+id/textView14"
            app:layout_constraintStart_toStartOf="@+id/textView14"
            app:layout_constraintTop_toTopOf="@+id/textView14"
            android:layout_marginTop="16dp"
            app:layout_constraintHorizontal_bias="0.437">

        <EditText
                android:layout_width="@dimen/_160sdp"
                android:layout_height="@dimen/_31sdp"
                android:inputType="textMultiLine"
                android:background="@color/white"
                android:text="test"
                android:ems="10"
                android:textColor="@color/black"
                android:id="@+id/donateowner"
                android:fontFamily="sans-serif-condensed"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="44dp"
                app:layout_constraintTop_toBottomOf="@+id/donatename"
                android:layout_marginTop="12dp" />

        <EditText
                android:layout_width="@dimen/_160sdp"
                android:layout_height="@dimen/_31sdp"
                android:inputType="textMultiLine"
                android:background="@color/white"
                android:text="test"
                android:ems="10"
                android:textColor="@color/black"
                android:id="@+id/donateby"
                android:fontFamily="sans-serif-condensed"
                app:layout_constraintTop_toBottomOf="@+id/donateowner"
                app:layout_constraintStart_toEndOf="@+id/ewqe3w"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.081"
                android:layout_marginTop="20dp" />

        <EditText
                android:layout_width="@dimen/_90sdp"
                android:layout_height="@dimen/_31sdp"
                android:inputType="textMultiLine"
                android:background="@color/white"
                android:text="Donate Name:"
                android:textSize="18dp"
                android:ems="10"
                android:textColor="@color/black"
                android:id="@+id/ewqeew"
                android:fontFamily="sans-serif-condensed"
                app:layout_constraintTop_toBottomOf="@+id/imageView6"
                android:layout_marginTop="28dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/donatename"
                app:layout_constraintHorizontal_bias="1.0" />

        <EditText
                android:layout_width="@dimen/_90sdp"
                android:layout_height="@dimen/_31sdp"
                android:inputType="textMultiLine"
                android:background="@color/white"
                android:text="Donated To:"
                android:textSize="18dp"
                android:ems="10"
                android:textColor="@color/black"
                android:id="@+id/ewqew"
                android:fontFamily="sans-serif-condensed"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/donateowner"
                app:layout_constraintTop_toBottomOf="@+id/ewqeew"
                app:layout_constraintHorizontal_bias="1.0"
                android:layout_marginTop="12dp" />

        <EditText
                android:layout_width="@dimen/_90sdp"
                android:layout_height="@dimen/_31sdp"
                android:inputType="textMultiLine"
                android:background="@color/white"
                android:text="Donated by:"
                android:textSize="18dp"
                android:ems="10"
                android:textColor="@color/black"
                android:id="@+id/ewqe3w"
                android:fontFamily="sans-serif-condensed"
                app:layout_constraintTop_toBottomOf="@+id/ewqew"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="16dp"
                android:layout_marginTop="20dp" />

        <EditText
                android:layout_width="@dimen/_160sdp"
                android:layout_height="@dimen/_31sdp"
                android:inputType="textMultiLine"
                android:background="@color/white"
                android:text="test"
                android:ems="10"
                android:textColor="@color/black"
                android:id="@+id/donatename"
                android:fontFamily="sans-serif-condensed"
                android:layout_marginTop="28dp"
                app:layout_constraintTop_toBottomOf="@+id/imageView6"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="44dp" />

        <ImageView
                android:layout_width="@dimen/_250sdp"
                android:layout_height="@dimen/_150sdp"
                app:srcCompat="@drawable/donatebox"
                android:id="@+id/imageView6"
                android:scaleType="fitXY"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="16dp"
                app:layout_constraintHorizontal_bias="0.507" />

        <EditText
                android:layout_width="@dimen/_160sdp"
                android:layout_height="@dimen/_100sdp"
                android:inputType="textMultiLine"
                android:background="@android:color/transparent"
                android:padding="10dp"
                android:text="description"
                android:textColor="@color/black"
                android:id="@+id/petdescription"
                app:layout_constraintTop_toBottomOf="@+id/donateby"
                app:layout_constraintStart_toEndOf="@+id/ewqw"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                android:layout_marginTop="16dp" />

        <EditText
                android:layout_width="@dimen/_90sdp"
                android:layout_height="@dimen/_31sdp"
                android:inputType="textMultiLine"
                android:background="@color/white"
                android:text="Description:"
                android:textSize="18dp"
                android:ems="10"
                android:textColor="@color/black"
                android:id="@+id/ewqw"
                android:fontFamily="sans-serif-condensed"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/ewqe3w"
                app:layout_constraintBottom_toTopOf="@+id/deletebutton"
                android:layout_marginStart="20dp"
                app:layout_constraintVertical_bias="0.158" />

        <Button
                android:text="Done receive"
                android:background="@drawable/button"
                android:textColor="@color/white"
                android:layout_width="@dimen/_120sdp"
                android:layout_height="@dimen/_45sdp"
                android:id="@+id/deletebutton"
                app:layout_constraintTop_toBottomOf="@+id/petdescription"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
