<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="headerText"></style>

    <style name="parent"></style>

    <style name="loginParent"></style>


    <style name="MySearchViewStyle" parent="Widget.AppCompat.SearchView">
        <!-- Change text color here -->
        <item name="android:textColor">@color/black</item>
        <!-- Change hint text color here -->
        <item name="android:textColorHint">@color/black</item>
        <!-- Change the color of the search underline here -->
        <item name="android:colorControlNormal">@color/black</item>
        <!-- Change the color of the search underline when activated -->
        <item name="android:colorControlActivated">@color/black</item>

    </style>

    <style name="ThemeOverlay.SearchView" parent="">
        <!-- Text color -->
        <item name="android:editTextColor">@color/black</item>
        <!-- Hint text color -->
        <item name="android:textColorHint">@color/black</item>
    </style>


</resources>